import java.util.*;
class greater
{
    void max(int a,int b)
    {   System.out.print ("This method does not return anything and takes integers as its arguments");
        if(a>b)
        System.out.print (a+" is greater");
        else
        System.out.print (b+" is greater");
    }
    float max(float a,float b)
    {   System.out.print ("This method returns float and takes float as its arguments");
        if(a>b)
        return a;
        else
        return b;
    }
}
class Q40greatdemo
{
public static void main(String args[])
    {
        int m,n;
        Scanner s=new Scanner(System.in);
        System.out.println("Enter two integer numbers");
        m=s.nextInt();
        n=s.nextInt();
        greater ob=new greater();
        ob.max(m,n);
        System.out.println("Enter two floating point  numbers");
        float a=s.nextFloat();
        float b=s.nextFloat();
        System.out.print (ob.max(a,b)+" is greater");
    }}